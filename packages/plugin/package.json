{
  "name": "@krix/devtool-plugin",
  "version": "0.0.0-alpha.0",
  "description": "Krix DevTool Plugin",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/krixsoft/krix-devtool"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gulp:clear:dist": "gulp --gulpfile=./common.gulp.js clear:dist",
    "gulp:clear:test": "gulp --gulpfile=./common.gulp.js clear:test",
    "webpack:build": "webpack --config webpack.prod.js --progress --colors",
    "dev:build": "gulp --gulpfile=./build.gulp.js build",
    "dev:watch": "gulp --gulpfile=./watch-pkg.gulp.js watch",
    "dev:test": "gulp --gulpfile=./watch-pkg.gulp.js test:watch",
    "pkg:build": "npm run gulp:clear:dist && npm run webpack:build && gulp --gulpfile=./pkg.gulp.js pkg:prepublish"
  },
  "keywords": [
    "krix",
    "devtool",
    "plugin"
  ],
  "authors": [
    "Andrey Shuleiko <geass1000@gmail.com> (https://github.com/Geass1000)",
    "Artur Huziahmetov <artur7work@gmail.com> (https://github.com/Huziahmetov-Artur)",
    "Daniil Agaev <kreksalix@gmail.com> (https://github.com/Krelix1)"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.152",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.5",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "chai": "^4.2.0",
    "del": "^5.1.0",
    "eslint": "^7.1.0",
    "eslint-loader": "^4.0.2",
    "gulp": "^4.0.2",
    "gulp-eslint": "^6.0.0",
    "gulp-mocha": "^7.0.2",
    "gulp-replace": "^1.0.0",
    "gulp-run": "^1.7.1",
    "gulp-typescript": "^5.0.1",
    "mocha": "^7.2.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "terser-webpack-plugin": "^3.0.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "rxjs": "^6.5.5"
  }
}
